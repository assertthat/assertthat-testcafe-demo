{
  "name": "testcafe-cucumber-demo",
  "version": "0.0.1",
  "description": "An example integration of TestCafe into CucumberJS",
  "author": "AssertThat",
  "scripts": {
    "pretest": "call ./scripts/features.cmd",
    "test": "./node_modules/.bin/cucumber-js.cmd -f json:reports/cucumber_report.json",
